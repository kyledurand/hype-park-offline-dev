@font-face {
  font-family: 'DINWeb';
  src: url('{{ 'DINWeb.eot?#iefix' | asset_url }}') format('embedded-opentype'),
       url('{{ 'DINWeb.woff' | asset_url }}') format('woff'),
       url('{{ 'DINWeb.ttf' | asset_url }}')  format('truetype');
  font-weight: normal;
}
@font-face {
  font-family: 'DINWeb';
  src: url('{{ 'DINWeb-Medium.eot?#iefix' | asset_url }}') format('embedded-opentype'),
       url('{{ 'DINWeb-Medium.woff' | asset_url }}') format('woff'),
       url('{{ 'DINWeb-Medium.ttf' | asset_url }}')  format('truetype');
  font-weight: bold;
}
@font-face {
  font-family: 'DINWeb';
  src: url('{{ 'DINWeb-Light.eot?#iefix' | asset_url }}') format('embedded-opentype'),
       url('{{ 'DINWeb-Light.woff' | asset_url }}') format('woff'),
       url('{{ 'DINWeb-Light.ttf' | asset_url }}')  format('truetype');
  font-weight: lighter;
}
